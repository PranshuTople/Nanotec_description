<node args="motorrev_left_position_controller motorrev_right_position_controller joint_state_controller " name="controller_spawner" ns="Nanotec" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/Nanotec/joint_states"/>
</node>

</launch>